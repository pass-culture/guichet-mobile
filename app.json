{
  "expo": {
      "scheme": "acme",
      "web": {
          "bundler": "metro"
      },
      "name": "guichet-mobile",
      "slug": "guichet-mobile",
      "ios": {
          "requireFullScreen": true
      },
      "plugins": [
          [
              "expo-screen-orientation",
              {
                  "initialOrientation": "PORTRAIT_UP"
              }
          ],
          [
              "expo-barcode-scanner",
              {
                  "cameraPermission": "Autoriser $(PRODUCT_NAME) à accéder à la caméra pour scanner les QR Codes."
              }
          ]
      ]
  }
}
